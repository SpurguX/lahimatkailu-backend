            a, .paginate_button {
                transition:.2s;
            }
            
            .fade-scale {
                transform: scale(0.7);
                opacity: 0;
                -webkit-transition: all .15s linear;
                -o-transition: all .15s linear;
                transition: all .15s linear;
            }

            .fade-scale.in {
                opacity: 1;
                transform: scale(1);
            }

            .modal {
                text-align: center;
                padding: 0!important;
            }

            .modal:before {
                content: '';
                display: inline-block;
                height: 100%;
                vertical-align: middle;
                margin-right: -4px;
            }

            .modal-dialog {
                display: inline-block;
                text-align: left;
                vertical-align: middle;
            }
            
            .typeFilter {
                text-align: center;
                margin-bottom: 5px;
            }
            
